ngInfo = SUPPORTED_LANGUAGES.find((l) => l.code === lang);\n    44→  return langInfo?.locale ?? hr;\n    45→}\n    46→\n    47→/**\n    48→ * Get the current language from localStorage or return default\n    49→ */\n    50→export function getCurrentLanguage(): LanguageCode {\n    51→  if (typeof window === \"undefined\") return DEFAULT_LANGUAGE;\n    52→\n    53→  const stored = localStorage.getItem(LANGUAGE_STORAGE_KEY);\n    54→  if (stored && isValidLanguage(stored)) {\n    55→    return stored as LanguageCode;\n    56→  }\n    57→  return DEFAULT_LANGUAGE;\n    58→}\n    59→\n    60→/**\n    61→ * Set the current language in localStorage\n    62→ */\n    63→export function setCurrentLanguage(lang: LanguageCode): void {\n    64→  if (typeof window === \"undefined\") return;\n    65→  localStorage.setItem(LANGUAGE_STORAGE_KEY, lang);\n    66→  // Dispatch a custom event so components can react to language changes\n    67→  window.dispatchEvent(new CustomEvent(\"languagechange\", { detail: { language: lang } }));\n    68→}\n    69→\n    70→/**\n    71→ * Check if a string is a valid language code\n    72→ */\n    73→export function isValidLanguage(lang: string): lang is LanguageCode {\n    74→  return [\"hr\", \"en\", \"fr\", \"de\"].includes(lang);\n    75→}\n    76→\n    77→/**\n    78→ * Get all translations for a specific language\n    79→ */\n    80→export function getTranslations(lang: LanguageCode = DEFAULT_LANGUAGE): typeof hrTranslations {\n    81→  return translations[lang] ?? translations[DEFAULT_LANGUAGE];\n    82→}\n    83→\n    84→/**\n    85→ * Get a specific translation by dot-notation path\n    86→ * Example: t(\"nav.fleet\", \"en\") returns \"Fleet\"\n    87→ */\n    88→export function t(key: string, lang: LanguageCode = DEFAULT_LANGUAGE): string {\n    89→  const trans = translations[lang] ?? translations[DEFAULT_LANGUAGE];\n    90→  const keys = key.split(\".\");\n    91→\n    92→  let result: unknown = trans;\n    93→  for (const k of keys) {\n